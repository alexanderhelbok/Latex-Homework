$ENV{'TFMFONTS'}='./mytexmf/fonts/tfm//:' . $ENV{'TFMFONTS'};
$ENV{'VFFONTS'}='./mytexmf/fonts/vf//:' . $ENV{'VFFONTS'};
$ENV{'ENCFONTS'}='./mytexmf/fonts/enc//:' . $ENV{'ENCFONTS'};
